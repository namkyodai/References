@Article{Margreth2008,
  author    = {Stefan Margreth and Andrea Roth},
  title     = {Interaction of flexible rockfall barriers with avalanches and snow pressure},
  journal   = {Cold Regions Science and Technology},
  year      = {2008},
  volume    = {51},
  number    = {2–3},
  pages     = {168 - 177},
  note      = {International Snow Science Workshop (ISSW) 2006},
  issn      = {0165-232X},
  doi       = {http://dx.doi.org/10.1016/j.coldregions.2007.03.008},
  url       = {http://www.sciencedirect.com/science/article/pii/S0165232X07000596},
  abstract  = {Rockfall barriers are optimized to absorb high punctual impact energies.
	In mountain areas the barriers are also loaded by avalanches and
	snow pressure. Snowpack forces and dynamic avalanche pressures act
	over a much larger area and over longer time periods. Thus, if not
	properly designed, rockfall barriers can be damaged. In winter 2003–2006
	we investigated the interaction of flexible rockfall barriers with
	avalanches and snow pressure on a study site in Fieberbrunn, Austria
	and in other areas. In several locations the barriers successfully
	stopped small wet snow avalanches. However, the main problem turned
	out to be the insufficient retention capacity during the whole winter
	and the structural behaviour. The weakest points are the retaining
	ropes and the post foundations. For an appropriate design of the
	barrier the main input factors determining snow pressure and avalanche
	pressure have to be assessed. },
