@Article{Martani2012,
  author    = {Claudio Martani and David Lee and Prudence Robinson and Rex Britter and Carlo Ratti},
  title     = {ENERNET: Studying the dynamic relationship between building occupancy and energy consumption},
  journal   = {Energy and Buildings},
  year      = {2012},
  volume    = {47},
